<root main_tree_to_execute = "MainTree" >
	<BehaviorTree ID="MainTree">
		<Sequence>

			<SetBlackboard output_key="Initial_pose" value="1.337;-0.433;0.0" /> 
			<SetBlackboard output_key="Goal_a" value="1.106;0.129;0.0" /> 

			<Fallback>
				<Repeat num_cycles="10">
					<Sequence>
						<SubTree ID="MoveRobot" target="Initial_pose" />
						<SubTree ID="MoveRobot" target="Goal_a" />
					</Sequence>
				</Repeat>
        <!--Error report node-->
			</Fallback>
		</Sequence>
	</BehaviorTree>


	<BehaviorTree ID="MoveRobot">
		<Sequence name="SetGoal">
			<MoveBase goal="{target}" />
		</Sequence>
	</BehaviorTree>
 
</root>

