 <root main_tree_to_execute = "MainTree" >
   <BehaviorTree ID="MainTree">
     <Repeat num_cycles="3">
	     <Sequence name="root_sequence">
		     <WaitForButton/>
		     <SetBlackboard output_key="Initial_pose" value="0.379;0.155;-2.572" /> 
		     <SetBlackboard output_key="Goal_a" value="-0.617;-1.207;-2.265" /> 
		     <SetBlackboard output_key="Goal_b" value="-0.803;-0.493;-2.509" /> 
		     <SetBlackboard output_key="Goal_c" value="-0.012;0.334;-2.399" />
		     <SubTree ID="MoveRobot" target="Initial_pose" />
		     <SubTree ID="MoveRobot" target="Goal_a" />
		     <SubTree ID="MoveRobot" target="Goal_b" />
		     <SubTree ID="MoveRobot" target="Goal_c" />
       </Sequence>
     </Repeat>
   </BehaviorTree>


 <BehaviorTree ID="MoveRobot">
	 <Sequence name="SetGoal">
		 <MoveBase goal="{target}" />
	 </Sequence>
 </BehaviorTree>
 
</root>
