<root main_tree_to_execute = "MainTree">
    <BehaviorTree ID="MainTree">
        <Sequence>
            <SetBlackboard output_key="Goal_a" value="1.6830;-0.1947;0.0;1.0" />
            <SetBlackboard output_key="Goal_b" value="-0.4924;0.8746;-1.0;0.0" />
            <SetBlackboard output_key="Goal_c" value="0.0;0.0;0.0;1.0" />
            <Repeat num_cycles="10">
                <Sequence>
                    <SubTree ID="MoveRobot" target="Goal_a" />
                    <SubTree ID="MoveRobot" target="Goal_b" />
                </Sequence>
            </Repeat>                
        </Sequence>
    </BehaviorTree>

    <BehaviorTree ID="MoveRobot">
        <Sequence name="SetGoal">
            <MoveBase goal="{target}" />
        </Sequence>
    </BehaviorTree>
</root>